extern<mynamespace:sheep> void sheep(){
    execute<as @a at @s>{
        for(int i=0; i<10; i++){
            coordinate pos = 0 0 0;
            pos.type = ~;
            pos.x = randrange(-5.0f,5.0f);
            pos.y = randrange(0.0f,5.0f);
            pos.z = randrange(-5.0f,5.0f);
            summon(sheep! ${pos});
        }
    }
}

