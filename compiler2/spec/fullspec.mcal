class Leader{
  public bool iscorrupt;
  public int iscommunist;
}

class Napoleon : Animal, Leader{
    using Animal::iscommunist;
    static Napoleon create(){
        Napoleon self = new Napoleon;
        self.iscommunist = true;
        self.iscorrupt = true;
        return self;
    }
}

